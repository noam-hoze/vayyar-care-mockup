{
  "name": "vayyar-care-mockup",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "firebase": "^11.6.0",
    "gsap": "^3.12.7",
    "less-loader": "^11.1.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.5.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "pnpm compile-less && react-scripts start",
    "build": "pnpm compile-less && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "compile-less": "lessc src/styles/main.less src/styles/main.css",
    "watch-less": "less-watch-compiler src/styles src/styles main.less",
    "dev": "concurrently \"pnpm watch-less\" \"pnpm start\"",
    "deploy": "pnpm build && firebase deploy",
    "deploy:preview": "pnpm build && firebase hosting:channel:deploy preview"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "less": "^4.2.2",
    "less-watch-compiler": "^1.16.3",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.3",
    "typescript": "^5.8.3"
  }
}
